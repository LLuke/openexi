<?xml version="1.0"?>
<xsd:schema targetNamespace="urn:foo"
           xmlns:xsd="http://www.w3.org/2001/XMLSchema"
           xmlns:foo="urn:foo" xmlns:goo="urn:goo">

<xsd:element name="A">
  <xsd:simpleType>
    <xsd:restriction base="xsd:QName">
      <xsd:enumeration value="foo:A"/>
