<?xml version="1.0"?>
<xsd:schema targetNamespace="urn:foo"
           xmlns:xsd="http://www.w3.org/2001/XMLSchema"
           xmlns:foo="urn:foo">

<xsd:element name="B">
  <xsd:complexType>
    <xsd:sequence>
      <xsd:element ref="foo:C" />
    </xsd:sequence>
  </xsd:complexType>
